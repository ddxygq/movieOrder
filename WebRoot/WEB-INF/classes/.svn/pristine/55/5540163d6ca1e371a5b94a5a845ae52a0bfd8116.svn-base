package com.syyf.movieMgr.pojo;

public class Ad {
	private String ad_id; // 广告ID
	private String ad_img; // 广告图片地址
	private String ad_href; // 广告链接地址
	public String getAd_id() {
		return ad_id;
	}
	public void setAd_id(String ad_id) {
		this.ad_id = ad_id;
	}
	public String getAd_img() {
		return ad_img;
	}
	public void setAd_img(String ad_img) {
		this.ad_img = ad_img;
	}
	public String getAd_href() {
		return ad_href;
	}
	public void setAd_href(String ad_href) {
		this.ad_href = ad_href;
	}
	
	
}
